@model IEnumerable<Appointment>

@if(TempData["AppointmentDeleted"] != null)
{
    <p class="text-success">TempData["AppointmentDeleted"]</p>
}

@if(TempData["DueAppointmentsDeleted"] != null)
{
    <p class="text-success">TempData["DueAppointmentsDeleted"]</p>
}

<div class="row row-cols-1 row-cols-md-3 g-4">
    @foreach(var appointment in Model)
    {
        <partial name="_AppointmentCard" model="appointment" />
    }
</div>